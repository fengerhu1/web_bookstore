<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="sample.Shopcart" table="shopcart" schema="test">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
            <generator class="native" />
        </id>
        <property name="bookname">
            <column name="bookname" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="number">
            <column name="number" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="writer">
            <column name="writer" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="price">
            <column name="price" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="buyer">
            <column name="buyer" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>